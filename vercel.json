{
  "version": 2,
  "installCommand": "cd backend && npm install",
  "buildCommand": "cd backend && npm run build",
  "builds": [
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/backend/dist/index.js"
    },
    {
      "src": "/zip-bundles",
      "dest": "/backend/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/dist/index.js"
    }
  ]
}
